{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
        "tabs",
        "*://*/*",
        "storage",
        "activeTab",
        "notifications",
        "declarativeContent"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "icon/32.png",
            "48": "icon/48.png"
        }
    },
    "icons": {
      "32": "icon/32.png",
      "48": "icon/48.png"
    },
    "web_accessible_resources": [
    "*"
    ],
    "manifest_version": 2
}
